{
  "name": "gitinfo",
  "version": "0.0.1",
  "description": "A simple TS-Node CLI for fetching and storing github user info.",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "start": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:reset": "npm run db:down && npm run db:up -- --force-recreate --build",
    "db:logs": "docker compose logs -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsmefreitas/gitinfo.git"
  },
  "author": "Luis Freitas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itsmefreitas/gitinfo/issues"
  },
  "homepage": "https://github.com/itsmefreitas/gitinfo#readme",
  "devDependencies": {
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "axios": "1.4.0",
    "dotenv": "16.1.3",
    "pg-promise": "11.5.0"
  }
}